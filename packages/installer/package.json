{
  "name": "@pie-cli-libs/installer",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "yarn": "^1.3.2"
  },
  "devDependencies": {
    "jest": "^22.1.4",
    "typescript": "^2.6.2",
    "@types/jest": "^22.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__test__/**/*.ts"
    ]
  }
}