{
  "name": "@pie-cli-libs/installer",
  "version": "0.2.3",
  "description": "installer for pie elements",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9.4.0",
    "npm": ">=5.6.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest unit",
    "integration-test": "./node_modules/.bin/jest int",
    "build": "rm -fr lib && ./node_modules/.bin/tsc --p tsconfig.build.json"
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.0.0",
    "cross-spawn": "^6.0.3",
    "find-up": "^2.1.0",
    "fs-extra": "^5.0.0",
    "global": "^4.3.2",
    "lodash": "^4.17.4",
    "log-factory": "^1.0.1",
    "npm-check-updates": "^2.14.0",
    "semver": "^5.5.0",
    "yarn": "^1.3.2"
  },
  "devDependencies": {
    "@types/find-up": "^2.1.1",
    "@types/fs-extra": "^5.0.0",
    "@types/jest": "^22.1.0",
    "@types/lodash": "^4.14.96",
    "@types/temp": "^0.8.31",
    "jest": "^22.1.4",
    "temp": "^0.8.3",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts"
    ]
  }
}
