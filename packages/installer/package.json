{
  "name": "@pie-cli-libs/installer",
  "version": "0.0.1",
  "description": "installer for pie elements",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.0.0",
    "cross-spawn": "^6.0.3",
    "find-up": "^2.1.0",
    "fs-extra": "^5.0.0",
    "log-factory": "^1.0.1",
    "yarn": "^1.3.2"
  },
  "devDependencies": {
    "@types/find-up": "^2.1.1",
    "@types/fs-extra": "^5.0.0",
    "@types/jest": "^22.1.0",
    "@types/lodash": "^4.14.96",
    "@types/temp": "^0.8.31",
    "jest": "^22.1.4",
    "temp": "^0.8.3",
    "typescript": "^2.6.2"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__test__/**/*.ts"
